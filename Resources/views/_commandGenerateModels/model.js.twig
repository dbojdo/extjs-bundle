Ext.define('{{ appName }}.model.{{ extjsSubclass }}', {
	extend: 'Ext.data.Model',
	fields: [{% for key, field in fields %}{
		name : '{{ field.name }}',
		type : '{{ field.type }}'
	}{% if loop.last == false %},{% endif %}{% endfor %}],
	proxy : {
		appendId : false,
		type : 'rest',
		url : Routing.generate('{{ route_name }}'),
		writer : {
			xtype : 'json',
			writeAllField : false,
			allowSingle : false
		}
	}
});
