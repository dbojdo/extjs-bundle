{
	"name" : "webit/extjs-bundle",
	"description" : "Web-IT ExtJS Bundle",
	"type" : "symfony-bundle",
	"authors" : [{
			"name" : "Web-IT",
			"homepage" : "http://www.web-it.eu"
		}
	],
	"keywords" : [
		"web-it",
		"extjs"
	],
	"homepage" : "http://www.web-it.eu",
	"license" : "MIT",
	"require" : {
		"php" : ">=5.3.2",
	    "symfony/symfony": "^2.4.0|^3.0.0",
	    "webit/php-tools" : "*",
	    "friendsofsymfony/rest-bundle": "^1.6.0|^2.0.0",
	    "jms/serializer-bundle" : "^1.0.0|^2.0.0"
	},
	"require-dev": {
		"doctrine/orm": "^2.3"
	},
	"autoload" : {
		"psr-4" : {
			"Webit\\Bundle\\ExtJsBundle\\" : ""
		}
	},
	"repositories" : [{
			"type" : "composer",
			"url" : "http://packages.web-it.eu"
		}
	],
  	"minimum-stability": "dev",
	"prefer-stable": true,
	"suggest" : {
		"vela/icons-bundle" : "Icons used by Bundle"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
